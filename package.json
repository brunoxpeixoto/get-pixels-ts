{
  "name": "get-pixels-ts",
  "version": "1.0.0",
  "description": "Reads the pixels of an image as an ndarray",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "dependencies": {
    "data-uri-to-buffer": "0.0.3",
    "jpeg-js": "^0.4.1",
