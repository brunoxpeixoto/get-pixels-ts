    "pngjs": "^3.3.3",
    "through": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/mime-types": "^2.1.1",
    "@types/ndarray": "^1.0.11",
    "@types/omggif": "^1.0.3",
    "@types/pngjs": "^6.0.1",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
