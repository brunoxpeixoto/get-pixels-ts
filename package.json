{
  "name": "get-pixels-ts",
  "version": "1.0.3",
  "description": "Reads the pixels of an image as an ndarray",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "dependencies": {
    "data-uri-to-buffer": "4.0.0",
    "jpeg-js": "0.4.4",
    "mime-types": "2.1.35",
    "ndarray": "1.0.19",
    "ndarray-pack": "1.2.1",
    "node-bitmap": "0.0.1",
    "omggif": "1.0.10",
    "pngjs": "6.0.0",
    "through": "2.3.8"
  },
  "devDependencies": {
    "@types/jest": "28.1.7",
    "@types/mime-types": "2.1.1",
    "@types/ndarray": "1.0.11",
    "@types/omggif": "1.0.3",
    "@types/pngjs": "6.0.1",
    "jest": "28.1.3",
    "ts-jest": "28.0.8",
    "typescript": "4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "lib"
